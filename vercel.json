{
  "version": 2,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm start:dev",
  "framework": null,
  "cleanUrls": true,
  "env": {},
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ]
}
